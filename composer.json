{
    "name": "emilmoe/fullsearch",
    "type": "extension",
    "description": "A full page search screen.",
    "license": "proprietary",
    "keywords": [
        "Search"
    ],
    "authors": [{
        "name": "Emil Moe",
        "email": "emil@moegroup.dk",
        "homepage": "https://moegroup.dk"
    }],
    "require": {
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "php": "~7.2|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "EmilMoe\\FullSearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EmilMoe\\FullSearch\\FullSearchServiceProvider"
            ]
        }
    }
}
